      "jimp": "^0.16.1",
      "jsqr": "^1.4.0",
      "lint-staged": "^10.5.4",
      "mailosaur": "^8.3.0",
      "next-connect": "^0.10.1",
      "prettier": "^2.2.1",
      "ts-jest": "^27.0.7",
      "tsconfig-paths": "^3.14.1",
      "typescript": "^4.5.4",
      "wdio-chromedriver-service": "^7.3.2",
      "webdriverio": "^7.20.5"
    },
    "engines": {
      "node": "14.17.5",
      "npm": "6.14.14"
    }
