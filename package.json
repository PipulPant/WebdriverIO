      "@commitlint/cli": "^12.0.1",
      "@next/eslint-plugin-next": "^12.0.7",
      "@trivago/prettier-plugin-sort-imports": "^3.1.1",
      "@types/date-and-time": "^0.13.0",
      "@types/mocha": "^9.1.0",
      "@types/node": "^17.0.0",
      "@types/uuid": "^8.3.0",
      "@typescript-eslint/eslint-plugin": "^5.8.1",
      "@typescript-eslint/parser": "^5.8.1",
